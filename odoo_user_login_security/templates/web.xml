<odoo>
	<template id="assets_backend" inherit_id="web.assets_backend">
		<xpath expr="//link[last()]" position="after">
			<link rel="stylesheet" type="text/css" href="/odoo_user_login_security/static/src/css/dashboard.css"/>
			<link rel="stylesheet" type="text/css" href="/odoo_user_login_security/static/src/css/owl.carousel.min.css"/>
			<link rel="stylesheet" type="text/css" href="/odoo_user_login_security/static/src/css/owl.theme.default.min.css"/>
		</xpath>
		<xpath expr="//script[last()]" position="after">
			<script type="text/javascript" src="/odoo_user_login_security/static/src/js/dashboard.js"/>
			<script type="text/javascript" src="/odoo_user_login_security/static/src/js/jquery.mousewheel.min.js"/>
			<script type="text/javascript" src="/odoo_user_login_security/static/src/js/owl.carousel.min.js"/>
		</xpath>
	</template>
	<template id="assets_frontend" inherit_id="web.assets_frontend">
		<xpath expr="." position="inside">
			<script type="text/javascript" src="/odoo_user_login_security/static/src/js/login.js"/>
		</xpath>
	</template>
</odoo>
