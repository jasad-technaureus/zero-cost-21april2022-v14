<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="purchase_analysis_form_view" model="ir.ui.view">
            <field name="name">purchase.analysis.form.view</field>
            <field name="model">purchase.analysis</field>
            <field name="arch" type="xml">
                <form string="Properties categories">
                    <group col="4" string="General Information">
                        <field name="from_date" />
                        <field name="to_date" />

                    </group>
                    <footer>
                        <button name="action_open_window" string="OPEN ANALYSIS" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="purchase_analysis_act_window" model="ir.actions.act_window">
            <field name="name">Purchase Analysis</field>
            <field name="res_model">purchase.analysis</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

</odoo>