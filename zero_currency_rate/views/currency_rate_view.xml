<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_currency_rate_tree_custom_new" model="ir.ui.view">
        <field name="name">view.currency.rate.tree.custom.new</field>
        <field name="model">res.currency.rate</field>
        <field name="inherit_id" ref="base.view_currency_rate_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='rate']" position="after">
                <field name="main_currency_rate" digits="[12,12]"/>
            </xpath>
        </field>
    </record>
</odoo>